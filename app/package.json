{
  "name": "peekaview",
  "author": "Limtec GmbH <info@limtec.de>",
  "productName": "PeekaView",
  "version": "0.0.1",
  "description": "Simple screen sharing application",
  "homepage": "https://peekaview.de",
  "license": "AGPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/peekaview/peekaview.git"
  },
  "main": "./out/main/main.js",
  "scripts": {
    "preinstall": "node preinstall.js",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview -- --no-sandbox",
    "dev": "electron-vite dev -- --no-sandbox",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:linux:deb": "npm run build && electron-builder --linux deb"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@nut-tree-fork/nut-js": "4.2.2",
    "@panzoom/panzoom": "^4.5.1",
    "bootstrap": "^5.3.3",
    "electron-log": "^5.2.2",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.1.7",
    "engine.io-client": "^6.5.3",
    "i18next": "^23.16.5",
    "i18next-fs-backend": "^2.3.2",
    "koffi": "^2.9.2",
    "livekit-client": "2.1.5",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.8.1",
    "sweetalert2": "^11.14.4",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "ws": "^8.18.0",
    "xmlhttprequest-ssl": "^2.0.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.21.1",
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/typescript-definitions": "^9.0.0",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/node": "^22.7.4",
    "@types/simple-peer": "^9.11.8",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^16.0.3",
    "electron": "^31.0.2",
    "electron-builder": "^26.0.0-alpha.6",
    "electron-vite": "^2.3.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "pnpm": "^9.14.1",
    "prettier": "^3.3.2",
    "typescript": "~5.6.3",
    "vite": "^5.3.1",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^2.1.8",
    "vue-template-compiler": "^2.7.16",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "pnpm@9.14.1+sha512.7f1de9cffea40ff4594c48a94776112a0db325e81fb18a9400362ff7b7247f4fbd76c3011611c9f8ac58743c3dc526017894e07948de9b72052f874ee2edfdcd"
}